{
	"name": "@ipg24/b24jssdk-main",
	"version": "0.1.7",
	"author": "Kirill Antropov",
	"license": "MIT",
	"private": true,
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"js",
		"restapi",
		"REST",
		"API"
	],
	"homepage": "https://github.com/MSITETOP/b24jssdk/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MSITETOP/b24jssdk.git"
	},
	"sideEffects": false,
	"type": "module",
	"workspaces": [
		"packages/*",
		"docs"
	],
	"scripts": {
		"build": "npm-run-all -s build:*",
		"build:jssdk-unbuild": "npm run --workspace=@ipg24/b24jssdk build",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@types/dotenv": "^6.1.1",
		"npm-run-all": "^4.1.5",
		"vitepress": "^1.3.4"
	},
	"dependencies": {
		"dotenv": "^16.4.1"
	}
}
