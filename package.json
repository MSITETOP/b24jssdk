	"author": "Bitrix",
	"license": "MIT",
	"private": true,
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"restapi"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk.git"
	},
	"type": "module",
