{
	"name": "@bitrix24/b24jssdk",
	"version": "0.1.0",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"restapi"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk.git",
		"directory": "packages/@b24/jssdk"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"README.md",
		"LICENSE",
		"dist"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepack": "npm run build",
		"build": "npm-run-all -s build:*",
		"build:unbuild": "unbuild",
		"lint": "eslint ."
	},
	"dependencies": {
		"axios": "^1.7.7",
		"@types/qs": "^6.9.15",
		"qs": "^6.13.0"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"@types/node": "^22.5.3",
		"eslint": "^9.9.1",
		"typescript": "latest",
		"vitest": "^2.0.5",
		"jiti": "^2.0.0-beta.3",
		"unbuild": "^3.0.0-rc.7"
	},
	"resolutions": {}
}
