{
	"name": "@bitrix24/b24jssdk",
	"version": "0.1.0",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"restapi"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk.git",
		"directory": "packages/jssdk"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		"./package.json": {
			"default": "./package.json"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"browser": {
		"./dist/index.js": "./dist/index.browser.js"
	},
	"files": [
		"README.md",
		"LICENSE",
		"dist",
		"package.json"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepack": "npm run build",
		"build": "npm-run-all -s build:*",
		"build:unbuild": "unbuild",
		"lint:prettier": "eslint . && prettier -c .",
		"lint": "eslint .",
		"lint:fix": "automd && eslint . --fix && prettier -w ."
	},
	"dependencies": {
		"axios": "^1.7.7",
		"qs": "^6.13.0",
		"luxon": "^3.5.0",
		"protobufjs": "^7.4.0"
	},
	"devDependencies": {
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.5.3",
		"@types/qs": "^6.9.15",
		"eslint-config-unjs": "^0.4.1",
		"prettier": "^3.3.3",
		"npm-run-all": "^4.1.5",
		"typescript": "latest",
		"unbuild": "^3.0.0-rc.11",
		"vitest": "^2.0.5"
	},
	"resolutions": {}
}
