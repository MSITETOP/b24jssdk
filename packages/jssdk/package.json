{
	"name": "@bitrix24/b24jssdk",
	"version": "0.1.0",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"restapi"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk.git",
		"directory": "packages/jssdk"
	},
	"type": "module",
	"exports": {
		"./package.json": {
			"default": "./package.json"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./tools/*": {
			"types": "./dist/tools/*.d.ts",
			"default": "./dist/tools/*.mjs"
		},
		"./types/*": {
			"types": "./dist/types/*.d.ts",
			"default": "./dist/types/*.mjs"
		},
		"./types/crm": {
			"types": "./dist/types/crm/index.d.ts",
			"default": "./dist/types/crm/index.mjs"
		},
		"./logger/*": {
			"types": "./dist/logger/*.d.ts",
			"default": "./dist/logger/*.mjs"
		},
		"./core/*": {
			"types": "./dist/core/*.d.ts",
			"default": "./dist/core/*.mjs"
		},
		"./core/http/*": {
			"types": "./dist/core/http/*.d.ts",
			"default": "./dist/core/http/*.mjs"
		},
		"./core/language/*": {
			"types": "./dist/core/language/*.d.ts",
			"default": "./dist/core/language/*.mjs"
		},
		"./hook": {
			"types": "./dist/hook/index.d.ts",
			"default": "./dist/hook/index.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"README.md",
		"LICENSE",
		"dist",
		"package.json"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepack": "npm run build",
		"build": "npm-run-all -s build:*",
		"build:unbuild": "unbuild",
		"lint": "eslint ."
	},
	"dependencies": {
		"axios": "^1.7.7",
		"@types/qs": "^6.9.15",
		"qs": "^6.13.0"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"@types/node": "^22.5.3",
		"eslint": "^9.9.1",
		"typescript": "latest",
		"vitest": "^2.0.5",
		"jiti": "^2.0.0-beta.3",
		"unbuild": "^3.0.0-rc.7"
	},
	"resolutions": {}
}
